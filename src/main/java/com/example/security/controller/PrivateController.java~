package com.example.security.controller;

import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

@RestController
@RequestMapping("/auth")
public class PrivateController {

    @GetMapping
    public String helloWordPrivate() {
        return "Hello World! from private endpoint";
    }
    @GetMapping("/user")
    public String helloWordUserPrivate() {
        return "Hello World! from privateUser endpoint";
    }
    @GetMapping("/admin")
    public String helloWordAdminPrivate() {
        return "Hello World! from privateAdmin endpoint";
    }

}
